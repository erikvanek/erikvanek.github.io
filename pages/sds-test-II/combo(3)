function closeAlert(c){document.getElementById("alert-"+c).className+=" hidden";var d=c,b=readCookie("hiddenAlerts");if(null!==b){for(var a="",b=b.split("\x26"),e=0;e<b.length;e++)null!==document.getElementById("alert-"+b[e])&&b[e]!==c&&(a=""!==a?a+("\x26"+b[e]):b[e]);""!==a&&(d=a+"\x26"+c)}createCookie("hiddenAlerts",d,14)}function createCookie(c,d,b){var a="";b&&(a=new Date,a.setTime(a.getTime()+864E5*b),a="; expires\x3d"+a.toUTCString());document.cookie=c+"\x3d"+d+a+"; path\x3d/"}
function readCookie(c){c+="\x3d";for(var d=document.cookie.split(";"),b=0;b<d.length;b++){for(var a=d[b];" "===a.charAt(0);)a=a.substring(1,a.length);if(0===a.indexOf(c))return a.substring(c.length,a.length)}return null};
