{
  "name": "interior-product-discovery",
  "version": "0.1.0",
  "description": "AI-powered product discovery tool for Czech interior architects",
  "main": "src/api/server.js",
  "type": "module",
  "scripts": {
    "dev:api": "nodemon src/api/server.js",
    "dev:frontend": "cd src/frontend && yarn dev",
    "crawl:siko": "node src/crawler/siko.js",
    "crawl:siko:test": "node src/crawler/siko.js",
    "crawl:siko:full": "MAX_PRODUCTS=null node src/crawler/siko.js",
    "crawl:all": "node src/crawler/index.js",
    "generate:embeddings": "node src/embeddings/generate.js",
    "test": "jest",
    "test:api": "node tests/test-api.js",
    "test:moodboard": "node src/llm/testMoodboard.js",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\""
  },
  "keywords": [
    "interior-design",
    "ai",
    "product-search",
    "claude",
    "e-commerce"
  ],
  "author": "Erik VanÄ›k",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "playwright": "^1.56.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}
